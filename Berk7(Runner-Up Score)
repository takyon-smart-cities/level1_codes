    #purpose of this code is to find the second largest number in the list
if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().split()))
    a = max(arr)
    while a == max(arr):
        arr.remove(max(arr))
    print(max(arr))
    
    
    
    #https://www.hackerrank.com/challenges/find-second-maximum-number-in-a-list/problem
